{
  "manifest_version": 2,

  "name": "KASHAN UNIVERSITY HS LOGIN",
  "description": "Smart Hs Connecter",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
        "matches": ["http://*/*", "https://*/*"],
        "js": ["popup.js"]
  }],
  "permissions": [
    "activeTab",
    "<all_urls>","webRequest","webRequestBlocking","http://www.hs.kashanu.ac.ir/",
	"https://www.google.com/"
  ]
}
